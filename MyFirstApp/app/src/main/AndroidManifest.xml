<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kaesava.kalyanaraman.kshetrapalapuram.myfirstapp" >

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kaesava.kalyanaraman.kshetrapalapuram.myfirstapp.MyActivity"
            android:label="@string/title_activity_my" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kaesava.kalyanaraman.kshetrapalapuram.myfirstapp.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName=".MyActivity" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="kaesava.kalyanaraman.kshetrapalapuram.myfirstapp.MyActivity" />
        </activity>
    </application>

</manifest>
